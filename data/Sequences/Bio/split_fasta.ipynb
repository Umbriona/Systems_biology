   "source": [
    "from Bio import SeqIO\n",
    "import os\n",
    "\n",
    "file = \"mdh_bio_muts.fasta\"\n",
    "\n",
    "DIR = \"MDH_BIO_SEQS\"\n",
    "\n",
    "\n",
    "for rec in SeqIO.parse(file, \"fasta\"):\n",
    "    with open(os.path.join(DIR, f\"{rec.id}.fasta\"), \"w\") as f:\n",
    "        f.write(f\">{rec.id}\\n{str(rec.seq)}\\n\")"
   ]
